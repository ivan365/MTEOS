{
  "termScale": 2.0,
  "termHeight": 19,
  "termWidth": 51,
  "renderer": "AWT",
  "maxComputerCapacity": 2097152,
  "maximumFilesOpen": 128,
  "httpEnable": true,
  "httpWhitelist": [
    "*"
  ],
  "httpBlacklist": [
    "$private"
  ],
  "defaultComputerSettings": "",
  "restoreSession": false,
  "computerThreads": 1,
  "http": {
    "websocketEnabled": true,
    "max_requests": 16,
    "max_websockets": 4
  },
  "plugins": {
    "net.clgd.ccemux.plugins.builtin.AWTPlugin": {
      "enabled": true,
      "nativePaste": false
    },
    "net.clgd.ccemux.plugins.builtin.CCEmuXAPI": {
      "enabled": true
    },
    "net.clgd.ccemux.plugins.builtin.HDFontPlugin": {
      "enabled": true
    },
    "net.clgd.ccemux.plugins.builtin.JFXPlugin": {
      "enabled": true,
      "forceUtilityDecoration": false,
      "doubleFontScale": true
    },
    "net.clgd.ccemux.plugins.builtin.TRoRPlugin": {
      "enabled": true
    },
    "net.clgd.ccemux.plugins.builtin.peripherals.DefaultPeripheralsPlugin": {
      "enabled": true,
      "disk_capacity": 125000
    }
  }
}